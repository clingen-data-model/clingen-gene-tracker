{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "backpack/crud": "^6.0",
        "backpack/pro": "2.2.20",
        "backpack/revise-operation": "^2.0",
        "backpack/theme-coreuiv2": "^1.2",
        "box/spout": "^3.0",
        "lab404/laravel-impersonate": "^1.3",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.3",
        "laravel/legacy-factories": "^1.1",
        "laravel/sanctum": "^4.0",
        "laravel/slack-notification-channel": "^3.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "lorisleiva/laravel-actions": "^2.4",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "spatie/ignition": "^1.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.0",
        "venturecraft/revisionable": "^1.0",
        "zircote/swagger-php": "^5.0"
    },
    "require-dev": {
        "backpack/generators": "^4.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Profiling\\": "Profiling/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate:fresh --seed",
            "php artisan migrate:fresh --seed --database=testing",
            "npm install"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": ["php artisan storage:link --quiet"],
        "test": [
            "@php artisan clear-compiled",
            "@php artisan storage:link",
            "@php artisan config:clear",
            "@php artisan cache:clear",
            "@php artisan view:clear",
            "@php artisan test"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "backpack": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    }
}
