{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "backpack/crud": "^5.5",
        "backpack/pro": "^1.6",
        "backpack/revise-operation": "^1.0",
        "box/spout": "^3.0",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^10.28",
        "laravel/helpers": "^1.6",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "^11.5",
        "laravel/sanctum": "^3.2",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lorisleiva/laravel-actions": "^2.5",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-permission": "^5.8",
        "venturecraft/revisionable": "^1.40",
        "zircote/swagger-php": "^4.4"
    },
    "require-dev": {
        "backpack/generators": "^3.3",
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Profiling\\": "Profiling/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan passport:keys",
            "php artisan migrate:fresh --seed",
            "php artisan migrate:fresh --seed --database=testing",
            "npm install"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan passport:keys"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
